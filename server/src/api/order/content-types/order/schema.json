{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "CustomName": {
      "type": "text"
    },
    "OrderDate": {
      "type": "datetime",
      "default": "2024-09-19T17:00:00.000Z"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Processing",
        "Complete",
        "Cancelled"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    }
  }
}
